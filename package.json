{
  "name": "gems.api",
  "version": "1.0.0",
  "main": "src/server.ts",
  "scripts": {
    "init": "node build.js",
    "init:local": "node build.js local",
    "init:staging": "node build.js staging",
    "init:prod": "node build.js production",
    "start": "npm run init && ts-node-dev --respawn --transpile-only src/server.ts",
    "start:local": "npm run init:local && ts-node-dev --respawn --transpile-only src/server.ts local",
    "start:staging": "node build/server.js",
    "start:prod": "node build/server.js",
    "migrate": "npm run init && prisma migrate deploy",
    "migrate:local": "npm run init:local && prisma migrate deploy",
    "migrate:staging": "npm run init:staging && prisma migrate deploy",
    "migrate:prod": "npm run init:prod && prisma migrate deploy",
    "add-migration": "npm run init && prisma migrate dev",
    "add-migration:local": "npm run init && prisma migrate local",
    "prisma:generate": "npm run init && prisma generate",
    "prisma:generate:local": "npm run init:local && prisma generate",
    "prisma:generate:staging": "npm run init:staging && prisma generate",
    "prisma:generate:prod": "npm run init:prod && prisma generate",
    "build": "npm run init && prisma generate && prisma migrate deploy && tsc -p tsconfig.json",
    "build:local": "npm run init:local && prisma generate && prisma migrate deploy && tsc -p tsconfig.json",
    "build:staging": "npm run init:staging && prisma generate && prisma migrate deploy && tsc -p tsconfig.json",
    "build:prod": "npm run init:prod && prisma generate && prisma migrate deploy && tsc -p tsconfig.json",
    "prisma:seed": "ts-node --transpile-only prisma/seed.ts",
    "db:seed": "npx prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
